package bagissto.core;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int checkPrimeNumber() {
		for(int i=1;i<=100;i++) {
			if (i == 1) {
				return 0;
			}
			for (int j = 2; j<= i/2; i++)
			   {
			      if (j % i == 0)
			      return 0;
			   }
		}
		return 0;
	}

}
